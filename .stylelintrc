{
  "extends": "stylelint-config-standard",
  "syntax": "scss",
  "rules": {
    "at-rule-no-unknown": [
      true, {
        "ignoreAtRules": ["if", "else", "extend", "mixin", "include", "content", "for", "function", "return"]
      }
    ],
    "font-family-no-missing-generic-family-keyword": null,
    "indentation": 2
  }
}
